<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
	targetNamespace="http://caixa.gov.br/simtx/negocial/pagamento/v1/pagamento"
	xmlns:comuns="http://caixa.gov.br/simtx/comuns/comuns" 
	xmlns:pagamento="http://caixa.gov.br/simtx/negocial/pagamento/v1/pagamento">

	<xsd:import schemaLocation="../../../COMUNS/V1/Comuns_Simtx.xsd" namespace="http://caixa.gov.br/simtx/comuns/comuns"></xsd:import>

	<!-- Atualizar este XSD com cautela
		Alguns campos devem permanecer diferente do SIBAR
		Ex: FLAG_NOVA_COBRANCA;SITUACAO_CONTINGENCIA;PARTICIPANTE_DESTINATARIO;BENEFICIARIOS
	 -->

	<xsd:element name="CONSULTA_BOLETO_ENTRADA" type="pagamento:dados_entrada_Type" />
	<xsd:element name="CONSULTA_BOLETO_SAIDA" type="pagamento:dados_saida_Type" />
	<xsd:element name="INFORMACOES_COMPLEMENTARES" type="pagamento:informacoes_complementares_saida_Type" />
	<xsd:element name="DADOS_COMPROVANTE" type="pagamento:dados_Comprovante_Type" />

	<xsd:complexType name="dados_entrada_Type">
		<xsd:sequence>
			<xsd:element name="CONSULTA_BOLETO" type="pagamento:consulta_boleto_pagamento_entrada_Type" minOccurs="1" maxOccurs="1"></xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="dados_saida_Type">
		<xsd:sequence>
			<xsd:element name="CONSULTA_BOLETO" type="pagamento:consulta_boleto_pagamento_saida_Type" minOccurs="1" maxOccurs="1"></xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	
	<!-- ============================= CONSULTA_BOLETO_ENTRADA ======================= -->
	<xsd:complexType name="consulta_boleto_pagamento_entrada_Type">
		<xsd:sequence>
			<xsd:element name="DATA_MOVIMENTO" type="xsd:date" minOccurs="1" maxOccurs="1" />
			<xsd:element name="FORMA_LEITURA" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>1-Codigo Barras|2-Digitavel</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:int">
						<xsd:enumeration value="1"></xsd:enumeration>
						<xsd:enumeration value="2"></xsd:enumeration>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="CODIGO_BARRAS" minOccurs="0" maxOccurs="1">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="44" />
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="LINHA_DIGITAVEL" minOccurs="0" maxOccurs="1">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="47" />
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="UNIDADE" type="comuns:short_0_to_4_digits_format_type" minOccurs="1" maxOccurs="1" />
		</xsd:sequence>
	</xsd:complexType>
	<!-- ============================= FIM CONSULTA_BOLETO_ENTRADA ======================= -->


	<!-- ============================= CONSULTA_BOLETO_SAIDA ======================= -->
	<xsd:complexType name="consulta_boleto_pagamento_saida_Type">
    	<xsd:sequence>
			<xsd:element name="FLAG_HABITACAO" type="pagamento:FLAG_S_N_TYPE"
				minOccurs="0" maxOccurs="1">
			</xsd:element>
			<xsd:element name="NUMERO_CONTROLE_DDA" minOccurs="0"
				maxOccurs="1">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"></xsd:minLength>
						<xsd:maxLength value="20"></xsd:maxLength>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
    		<xsd:element name="TIPO_BOLETO" minOccurs="0"
    			maxOccurs="1">
    			<xsd:simpleType>
    				<xsd:restriction base="xsd:string">
    					<xsd:maxLength value="25"></xsd:maxLength>
    				</xsd:restriction>
    			</xsd:simpleType>
    		</xsd:element>
    		<xsd:element name="TITULO"
    			type="pagamento:titulo_Type" minOccurs="0"
    			maxOccurs="1">
    		</xsd:element>
    		<xsd:element name="QUANTIDADE_DIAS_PROTESTO" minOccurs="0"
    			maxOccurs="1">
    			<xsd:simpleType>
    				<xsd:restriction base="xsd:int">
    					<xsd:minInclusive value="0"></xsd:minInclusive>
    					<xsd:maxInclusive value="999999"></xsd:maxInclusive>
    				</xsd:restriction>
    			</xsd:simpleType>
    		</xsd:element>
    		<xsd:element name="DATA_LIMITE_PAGAMENTO" type="xsd:date"
    			minOccurs="0" maxOccurs="1">
    		</xsd:element>
    		<xsd:element name="RECEBE_VALOR_DIVERGENTE" minOccurs="0"
    			maxOccurs="1">
    			<xsd:simpleType>
    				<xsd:restriction base="xsd:string">
    					<xsd:enumeration value="QUALQUER_VALOR"></xsd:enumeration>
    					<xsd:enumeration value="ENTRE_MINIMO_MAXIMO"></xsd:enumeration>
    					<xsd:enumeration value="NAO_ACEITAR"></xsd:enumeration>
    					<xsd:enumeration value="SOMENTE_VALOR_MINIMO"></xsd:enumeration>
    				</xsd:restriction>
    			</xsd:simpleType>
    		</xsd:element>
    		<xsd:element name="FLAG_RECEBIMENTO_CHEQUE"
    			type="pagamento:FLAG_S_N_TYPE" minOccurs="0"
    			maxOccurs="1">
    		</xsd:element>
    		<xsd:element name="FLAG_BLOQUEIO_PAGAMENTO" minOccurs="0"
    			maxOccurs="1" type="pagamento:FLAG_S_N_TYPE">
    		</xsd:element>
    		<xsd:element name="FLAG_PAGAMENTO_PARCIAL"
    			type="pagamento:FLAG_S_N_TYPE" minOccurs="0"
    			maxOccurs="1">
    		</xsd:element>
    		<xsd:element name="QUANTIDADE_PAGAMENTO_PARCIAL_PERMITIDO"
    			minOccurs="0" maxOccurs="1">
    			<xsd:simpleType>
    				<xsd:restriction base="xsd:short">
    					<xsd:minInclusive value="0"></xsd:minInclusive>
    					<xsd:maxInclusive value="999"></xsd:maxInclusive>
    				</xsd:restriction>
    			</xsd:simpleType>
    		</xsd:element>
    		<xsd:element name="QUANTIDADE_PAGAMENTO_PARCIAL_REGISTRADO"
    			minOccurs="0" maxOccurs="1">
    			<xsd:simpleType>
    				<xsd:restriction base="xsd:short">
    					<xsd:maxInclusive value="999"></xsd:maxInclusive>
    					<xsd:minInclusive value="0"></xsd:minInclusive>
    				</xsd:restriction>
    			</xsd:simpleType>
    		</xsd:element>
    		<xsd:element name="VALOR_SALDO_TOTAL_ATUAL"
    			type="pagamento:VALOR_TYPE" minOccurs="0"
    			maxOccurs="1">
    		</xsd:element>
    		<xsd:element name="MODELO_CALCULO" minOccurs="0"
    			maxOccurs="1">
    			<xsd:simpleType>
    				<xsd:restriction base="xsd:string">
    					<xsd:length value="2"></xsd:length>
    				</xsd:restriction>
    			</xsd:simpleType>
    		</xsd:element>
    		<xsd:element name="CALCULOS"
    			type="pagamento:calculos_Type" minOccurs="0"
    			maxOccurs="1">
    		</xsd:element>
    		<xsd:element name="BAIXAS"
    			type="pagamento:baixas_Type" minOccurs="0"
    			maxOccurs="1">
    		</xsd:element>
    		<xsd:element name="DATA_HORA_DDA" type="xsd:dateTime"
    			minOccurs="0" maxOccurs="1">
    		</xsd:element>
    		<xsd:element name="DATA_MOVIMENTO" type="xsd:date"
    			minOccurs="0" maxOccurs="1">
    		</xsd:element>
    		<xsd:element name="FLAG_ULTIMA_PARCELA_VIAVEL"
    			type="pagamento:FLAG_S_N_TYPE" minOccurs="0"
    			maxOccurs="1">
    		</xsd:element>
    		<xsd:element name="FLAG_RECEBER_QUALQUER_VALOR"
    			type="pagamento:FLAG_S_N_TYPE" minOccurs="0"
    			maxOccurs="1">
    		</xsd:element>
    		<xsd:element name="NUMERO_PARCELA_ATUAL" minOccurs="0"
    			maxOccurs="1">
    			<xsd:simpleType>
    				<xsd:restriction base="xsd:short">
    					<xsd:minExclusive value="0"></xsd:minExclusive>
    					<xsd:maxInclusive value="999"></xsd:maxInclusive>
    				</xsd:restriction>
    			</xsd:simpleType>
    		</xsd:element>
    	</xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="pessoa_comum_Type">
    	<xsd:choice minOccurs="1" maxOccurs="1">
    		<xsd:sequence>
    			<xsd:element name="CPF" minOccurs="1" maxOccurs="1">
    				<xsd:simpleType>
    					<xsd:restriction base="xsd:long">
    						<xsd:minInclusive value="0"></xsd:minInclusive>
    						<xsd:maxInclusive value="99999999999"></xsd:maxInclusive>
    					</xsd:restriction>
    				</xsd:simpleType>
    			</xsd:element>
    			<xsd:element name="NOME" minOccurs="1" maxOccurs="1" type="pagamento:NOME_TYPE">
    			</xsd:element>
    		</xsd:sequence>
    		<xsd:sequence>
    			<xsd:element name="CNPJ" minOccurs="1" maxOccurs="1">
    				<xsd:simpleType>
    					<xsd:restriction base="xsd:long">
    						<xsd:minInclusive value="0"></xsd:minInclusive>
    						<xsd:maxInclusive value="99999999999999"></xsd:maxInclusive>
    					</xsd:restriction>
    				</xsd:simpleType>
    			</xsd:element>
    			<xsd:element name="RAZAO_SOCIAL" minOccurs="1"
    				maxOccurs="1">
    				<xsd:simpleType>
    					<xsd:restriction base="xsd:string">
    						<xsd:minLength value="1"></xsd:minLength>
    						<xsd:maxLength value="50"></xsd:maxLength>
    					</xsd:restriction>
    				</xsd:simpleType>
    			</xsd:element>
    			<xsd:element name="NOME_FANTASIA" minOccurs="0"
    				maxOccurs="1">
    				<xsd:simpleType>
    					<xsd:restriction base="xsd:string">
    						<xsd:maxLength value="50"></xsd:maxLength>
    					</xsd:restriction>
    				</xsd:simpleType>
    			</xsd:element>
    		</xsd:sequence>
    		<xsd:sequence>
    			<xsd:element name="PIS" minOccurs="1" maxOccurs="1">
    				<xsd:simpleType>
    					<xsd:restriction base="xsd:long">
    						<xsd:maxInclusive value="99999999999"></xsd:maxInclusive>
    						<xsd:minExclusive value="0"></xsd:minExclusive>
    					</xsd:restriction>
    				</xsd:simpleType>
    			</xsd:element>
    			<xsd:element name="NOME" minOccurs="1" maxOccurs="1" type="pagamento:NOME_TYPE">
    			</xsd:element>
    		</xsd:sequence>
    		<xsd:sequence>
    			<xsd:element name="IDENTIFICACAO" minOccurs="1" maxOccurs="1">
    				<xsd:simpleType>
    					<xsd:restriction base="xsd:string">
    						<xsd:maxLength value="15"></xsd:maxLength>
    						<xsd:minLength value="1"></xsd:minLength>
    					</xsd:restriction>
    				</xsd:simpleType>
    			</xsd:element>
    			<xsd:element name="NOME" minOccurs="1" maxOccurs="1" type="pagamento:NOME_TYPE">
    			</xsd:element>
    		</xsd:sequence>
    	</xsd:choice>
    </xsd:complexType>
    
    <xsd:complexType name="beneficiario_original_Type">
    	<xsd:complexContent>
    		<xsd:extension
    			base="pagamento:pessoa_comum_Type">
    			<xsd:sequence>
                    <xsd:element name="CODIGO" minOccurs="0"
                    	maxOccurs="1">
                    	<xsd:simpleType>
                    		<xsd:restriction base="xsd:long">
                    			<xsd:minInclusive value="0"></xsd:minInclusive>
                    			<xsd:maxInclusive
                    				value="999999999999999">
                    			</xsd:maxInclusive>
                    		</xsd:restriction>
                    	</xsd:simpleType>
                    </xsd:element>
                    <xsd:element name="DV" minOccurs="0"
                    	maxOccurs="1">
                    	<xsd:simpleType>
                    		<xsd:restriction base="xsd:short">
                    			<xsd:minInclusive value="0"></xsd:minInclusive>
                    			<xsd:maxInclusive value="9"></xsd:maxInclusive>
                    		</xsd:restriction>
                    	</xsd:simpleType>
                    </xsd:element>
                    <xsd:element name="LOGRADOURO" minOccurs="0"
    					maxOccurs="1">
    					<xsd:simpleType>
    						<xsd:restriction base="xsd:string">
    							<xsd:maxLength value="40"></xsd:maxLength>
    						</xsd:restriction>
    					</xsd:simpleType>
    				</xsd:element>
    				<xsd:element name="CIDADE" minOccurs="0"
    					maxOccurs="1">
    					<xsd:simpleType>
    						<xsd:restriction base="xsd:string">
    							<xsd:maxLength value="50"></xsd:maxLength>
    						</xsd:restriction>
    					</xsd:simpleType>
    				</xsd:element>
    				<xsd:element name="UF" minOccurs="0"
    					maxOccurs="1">
    					<xsd:simpleType>
    						<xsd:restriction base="xsd:string">
    							<xsd:length value="2"></xsd:length>
    						</xsd:restriction>
    					</xsd:simpleType>
    				</xsd:element>
    				<xsd:element name="CEP" minOccurs="0"
    					maxOccurs="1">
    					<xsd:simpleType>
    						<xsd:restriction base="xsd:int">
    							<xsd:minInclusive value="0"></xsd:minInclusive>
    							<xsd:maxInclusive value="99999999"></xsd:maxInclusive>
    						</xsd:restriction>
    					</xsd:simpleType>
    				</xsd:element>
    			</xsd:sequence>
    		</xsd:extension>
    	</xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="beneficiario_final_Type">
    	<xsd:complexContent>
    		<xsd:extension
    			base="pagamento:pessoa_comum_Type">
    		</xsd:extension>
    	</xsd:complexContent>
    </xsd:complexType>
    
    <xsd:complexType name="pagador_Type">
    	<xsd:complexContent>
    		<xsd:extension
    			base="pagamento:pessoa_comum_Type">
    		</xsd:extension>
    	</xsd:complexContent>
    </xsd:complexType>
    


    <xsd:complexType name="valores_aplicar_Type">
    	<xsd:sequence>
    		<xsd:element name="DATA" type="xsd:date" minOccurs="0" maxOccurs="1"></xsd:element>
    		<xsd:choice minOccurs="1" maxOccurs="1">
    			<xsd:element name="VALOR" minOccurs="1" maxOccurs="1">
	    			<xsd:simpleType>
	    				<xsd:restriction base="xsd:decimal">
	    					<xsd:totalDigits value='17' />
	    					<xsd:fractionDigits value='5' />
	    				</xsd:restriction>
	    			</xsd:simpleType>
	    		</xsd:element>
    			<xsd:element name="PERCENTUAL" minOccurs="1"
    				maxOccurs="1" type="pagamento:PERCENTUAL_VALOR_TYPE">
    			</xsd:element>
    		</xsd:choice>
    	</xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="calculos_Type">
    	<xsd:sequence>
    		<xsd:element name="CALCULO_CIP" type="pagamento:calculo_Type" minOccurs="0" maxOccurs="unbounded"></xsd:element>
    		<xsd:element name="CALCULO_CAIXA" type="pagamento:calculo_caixa_Type" minOccurs="0" maxOccurs="5"></xsd:element>
    	</xsd:sequence>
    </xsd:complexType>
    
    <xsd:complexType name="calculo_Type">
    	<xsd:sequence>
    		<xsd:element name="VALOR_JUROS" type="pagamento:VALOR_TYPE" minOccurs="1" maxOccurs="1"></xsd:element>
    		<xsd:element name="VALOR_MULTA" type="pagamento:VALOR_TYPE" minOccurs="1" maxOccurs="1"></xsd:element>
    		<xsd:element name="VALOR_DESCONTO" type="pagamento:VALOR_TYPE" minOccurs="1" maxOccurs="1"></xsd:element>
    		<xsd:element name="VALOR_TOTAL" type="pagamento:VALOR_TYPE" minOccurs="1" maxOccurs="1"></xsd:element>
    		<xsd:element name="DATA_VALIDADE_CALCULO" type="xsd:date" minOccurs="1" maxOccurs="1"></xsd:element>
    	</xsd:sequence>
    </xsd:complexType>
    
    <xsd:complexType name="baixa_operacional_Type">
    	<xsd:sequence>
    		<xsd:element name="NUMERO_IDENTIFICACAO" minOccurs="0"
    			maxOccurs="1">
    			<xsd:simpleType>
    				<xsd:restriction base="xsd:unsignedLong">
    					<xsd:minInclusive value="0"></xsd:minInclusive>
    					<xsd:maxInclusive value="9999999999999999999"></xsd:maxInclusive>
    				</xsd:restriction>
    			</xsd:simpleType>
    		</xsd:element>
    		<xsd:element name="NUMERO_REFERENCIA_ATUAL" minOccurs="0"
    			maxOccurs="1"
    			type="pagamento:NUM_SEQ_CADASTRO_DDA_TYPE">
    		</xsd:element>
    		<xsd:element name="NUMERO_SEQUENCIA_ATUALIZACAO"
    			type="pagamento:NUM_SEQ_CADASTRO_DDA_TYPE"
    			minOccurs="0" maxOccurs="1">
    		</xsd:element>
    		<xsd:element name="DATA_PROCESSAMENTO" type="xsd:date"
    			minOccurs="0" maxOccurs="1">
    		</xsd:element>
    		<xsd:element name="DATA_HORA_PROCESSAMENTO"
    			type="xsd:dateTime" minOccurs="0" maxOccurs="1">
    		</xsd:element>
    		<xsd:element name="DATA_HORA_SITUACAO" type="xsd:dateTime"
    			minOccurs="0" maxOccurs="1">
    		</xsd:element>
    		<xsd:element name="VALOR_TITULO"
    			type="pagamento:VALOR_TYPE" minOccurs="0"
    			maxOccurs="1">
    		</xsd:element>
    		<xsd:element name="CODIGO_BARRAS" minOccurs="0"
    			maxOccurs="1">
    			<xsd:simpleType>
    				<xsd:restriction base="xsd:string">
    					<xsd:minLength value="1"></xsd:minLength>
    					<xsd:maxLength value="44"></xsd:maxLength>
    				</xsd:restriction>
    			</xsd:simpleType>
    		</xsd:element>
    	</xsd:sequence>
    </xsd:complexType>
    
    <xsd:complexType name="baixa_efetiva_Type">
    	<xsd:complexContent>
    		<xsd:extension
    			base="pagamento:baixa_operacional_Type">
    			<xsd:sequence>
    				<xsd:element name="CANAL_PAGAMENTO" minOccurs="0"
    					maxOccurs="1">
    					<xsd:simpleType>
    						<xsd:restriction base="xsd:string">
    							<xsd:enumeration value="AGENCIA"></xsd:enumeration>
    							<xsd:enumeration
    								value="AUTO_ATENDIMENTO">
    							</xsd:enumeration>
    							<xsd:enumeration value="INTERNET"></xsd:enumeration>
    							<xsd:enumeration
    								value="CORRESPONDENTE">
    							</xsd:enumeration>
    							<xsd:enumeration
    								value="CENTRAL_ATENDIMENTO">
    							</xsd:enumeration>
    							<xsd:enumeration
    								value="ARQUIVO_ELETRONICO">
    							</xsd:enumeration>
    							<xsd:enumeration value="DDA"></xsd:enumeration>
    						</xsd:restriction>
    					</xsd:simpleType>
    				</xsd:element>
    				<xsd:element name="MEIO_PAGAMENTO" minOccurs="0"
    					maxOccurs="1">
    					<xsd:simpleType>
    						<xsd:restriction base="xsd:string">
    							<xsd:enumeration value="ESPECIE"></xsd:enumeration>
    							<xsd:enumeration value="DEBITO_CONTA"></xsd:enumeration>
    							<xsd:enumeration
    								value="CARTAO_CREDITO">
    							</xsd:enumeration>
    							<xsd:enumeration value="CHEQUE"></xsd:enumeration>
    						</xsd:restriction>
    					</xsd:simpleType>
    				</xsd:element>
    			</xsd:sequence>
    		</xsd:extension>
    	</xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="baixas_Type">
    	<xsd:sequence>
    		<xsd:element name="BAIXA_OPERACIONAL" type="pagamento:baixa_operacional_Type" minOccurs="0" maxOccurs="unbounded"></xsd:element>
    		<xsd:element name="BAIXA_EFETIVA" type="pagamento:baixa_efetiva_Type" minOccurs="0" maxOccurs="unbounded"></xsd:element>
    	</xsd:sequence>
    </xsd:complexType>
    
    <xsd:complexType name="juros_aplicar_Type">
    	<xsd:complexContent>
    		<xsd:extension
    			base="pagamento:valores_aplicar_Type">
    			<xsd:sequence>
    				<xsd:element name="TIPO" minOccurs="1"
    					maxOccurs="1">
    					<xsd:simpleType>
    						<xsd:restriction base="xsd:string">
    							<xsd:enumeration
    								value="VALOR_DIA_CORRIDO">
    							</xsd:enumeration>
    							<xsd:enumeration
    								value="PERCENTUAL_DIA_CORRIDO">
    							</xsd:enumeration>
    							<xsd:enumeration
    								value="PERCENTUAL_MES_DIA_CORRIDO">
    							</xsd:enumeration>
    							<xsd:enumeration
    								value="PERCENTUAL_ANO_DIA_CORRIDO">
    							</xsd:enumeration>
    							<xsd:enumeration value="ISENTO"></xsd:enumeration>
    							<xsd:enumeration
    								value="VALOR_DIA_UTIL">
    							</xsd:enumeration>
    							<xsd:enumeration
    								value="PERCENTUAL_DIA_UTIL">
    							</xsd:enumeration>
    							<xsd:enumeration
    								value="PERCENTUAL_MES_DIA_UTIL">
    							</xsd:enumeration>
    							<xsd:enumeration value="PERCENTUAL_ANO_DIA_UTIL"></xsd:enumeration>
    						</xsd:restriction>
    					</xsd:simpleType>
    				</xsd:element>
    			</xsd:sequence>
    		</xsd:extension>
    	</xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="multa_aplicar_Type">
    	<xsd:complexContent>
    		<xsd:extension
    			base="pagamento:valores_aplicar_Type">
    			<xsd:sequence>
    				<xsd:element name="TIPO" minOccurs="1"
    					maxOccurs="1">
    					<xsd:simpleType>
    						<xsd:restriction base="xsd:string">
    							<xsd:enumeration value="VALOR_FIXO"></xsd:enumeration>
    							<xsd:enumeration value="PERCENTUAL"></xsd:enumeration>
    							<xsd:enumeration value="ISENTO"></xsd:enumeration>
    						</xsd:restriction>
    					</xsd:simpleType>
    				</xsd:element>
    			</xsd:sequence>
    		</xsd:extension>
    	</xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="desconto_aplicar_Type">
