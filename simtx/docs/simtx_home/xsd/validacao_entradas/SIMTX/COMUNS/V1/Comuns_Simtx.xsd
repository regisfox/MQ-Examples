   <xsd:simpleType name="short_0_to_2_digits_format_type">
		<xsd:restriction base="xsd:short">
			<xsd:minInclusive value="0" />
			<xsd:maxInclusive value="99" />
		</xsd:restriction>
	</xsd:simpleType>
  
  	<xsd:simpleType name="short_0_to_3_digits_format_type">
		<xsd:restriction base="xsd:short">
			<xsd:minInclusive value="0" />
			<xsd:maxInclusive value="999" />
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="short_0_to_4_digits_format_type">
		<xsd:restriction base="xsd:short">
			<xsd:minInclusive value="0" />
			<xsd:maxInclusive value="9999" />
		</xsd:restriction>
	</xsd:simpleType>
	
	<xsd:simpleType name="valor_decimal_format_type">
		<xsd:restriction base="xsd:decimal">
			<xsd:minInclusive value="-9999999999999.99" />
			<xsd:maxInclusive value="9999999999999.99" />
			<xsd:fractionDigits value="2" />
			<xsd:totalDigits value="15" />
		</xsd:restriction>
	</xsd:simpleType>
	
	<xsd:simpleType name="valor_decimal_format_type_17_2">
		<xsd:restriction base="xsd:decimal">
			<xsd:minInclusive value="-999999999999999.99" />
			<xsd:maxInclusive value="999999999999999.99" />
			<xsd:fractionDigits value="2" />
			<xsd:totalDigits value="17" />
		</xsd:restriction>
	</xsd:simpleType>
	
