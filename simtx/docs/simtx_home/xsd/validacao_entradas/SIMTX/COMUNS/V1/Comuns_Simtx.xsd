	      <xsd:element minOccurs="1" name="ACAO" type="comuns:short_0_to_2_digits_format_type" />
	      <xsd:element minOccurs="1" name="MODO" type="comuns:short_0_to_2_digits_format_type" />
	      <xsd:element minOccurs="1" name="GRUPO" type="comuns:short_0_to_2_digits_format_type" />
	      <xsd:element minOccurs="1" name="SUBGRUPO" type="comuns:short_0_to_3_digits_format_type" />
	      <xsd:element minOccurs="1" name="TIPO" type="comuns:short_0_to_4_digits_format_type" />
	      <xsd:element minOccurs="1" name="SUBTIPO" type="comuns:short_0_to_4_digits_format_type" />
	    </xsd:sequence>
