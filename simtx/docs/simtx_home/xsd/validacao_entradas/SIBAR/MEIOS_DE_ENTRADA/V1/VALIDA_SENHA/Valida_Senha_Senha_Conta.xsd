						CAMPO\sCONHECIDO\sNO\sISO\sCOMO\sBIT123.\r\n\r\nNOS\sDADOS\sDE\sENTRADA\sESTE\sCAMPO\sSÓ\sÉ\sENVIADO\sQUANDO\sA\sOPERAÇÃO\sFOR\s"INFORMA_ERRO_IDENTIFICACAO_POSITIVA".
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="400" />
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="DADOS_DERIVACAO_CHAVE" minOccurs="0"
				maxOccurs="1">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="24"></xsd:maxLength>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
