			</xsd:element>
			<xsd:element minOccurs="0" name="TAMANHO_INFORMACOES_TELA">
				<xsd:annotation>
					<xsd:documentation source="WMQI_DOCUMENTATION">TAMANHO\sDO\sCAMPO\s"INFORMACOES_TELA".
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:short">
						<xsd:minInclusive value="0" />
						<xsd:maxInclusive value="999" />
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element minOccurs="0" name="CRIPTOGRAMA">
				<xsd:annotation>
					<xsd:documentation source="WMQI_DOCUMENTATION">CAMPO\sCONHECIDO\sNO\sISO\sCOMO\sBIT112.\r\n\r\nSERÁ\sDEVOLVIDO\sCOM\sOS\sDADOS\sPARA\sCHECAGEM\sDA\sIDENTIFICAÇÃO\sPOSITIVA\sPELO\sCANAL\sORIGEM.
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="100" />
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element minOccurs="0" name="TAMANHO_CRIPTOGRAMA">
				<xsd:annotation>
					<xsd:documentation source="WMQI_DOCUMENTATION">TAMANHO\sDO\sCAMPO\s"CRIPTOGRAMA".
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:short">
						<xsd:minInclusive value="0" />
						<xsd:maxInclusive value="999" />
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element minOccurs="0" name="FLAG_ULTIMA_TENTATIVA">
				<xsd:annotation>
					<xsd:documentation source="WMQI_DOCUMENTATION">INDICA\sSE\sEH\sA\sULTIMA\sTENTATIVA\sPARA\sO\sCLIENTE\sACERTAR\sA\sIP:\r\n\r\nDomínio:\r\nS\s-\sULTIMA\sTENTATIVA.\r\nN\s-\sNAO\sÉ\sA\sULTIMA\sTENTATIVA.
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="1" />
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element minOccurs="0" name="TP_IDENTIFICACAO_POSITIVA">
				<xsd:annotation>
					<xsd:documentation source="WMQI_DOCUMENTATION">Este\scampo\sNÃO\sdeve\sser\sverificado\sno\scaso\sdo\scampo\sFLAG_VALIDACAO\sser\sigual\sa\s'N'.\r\n\r\nDomínio:\r\n1\s-\sO\sCRIPTOGRAMA\snão\sserá\sfornecido.\sO\sprocesso\sde\sautorizacao\sda\stransacao\sdeve\sser\sINTERROMPIDO\se\sdeverá\sser\sdevolvido\s\spara\so\scanal\sde\sorigem\so\scampo\sINFORMACOES_TELA\se\suma\sinformacao\sque\sindique\sque\sa\sIdentificação\sPositiva\sdeve\sser\scolhida\sdo\scliente\se\senviada\spara\schegagem\sno\smainframe\s(neste\scaso\sa\sIdentificação\sPositiva\sdeve\socorrer\sOBRIGATORIAMENTE\sno\sSIRAN.\r\n2\s-\sO\sCRIPTOGRAMA\sserá\sfornecido\spelo\sSIRAN\spara\spossibilitar\sa\schecagem\sda\sIdentificação\sPositiva\spelo\scanal\sde\sorigem.\r\n3\s-\sSerá\snecessário\sefetuar\scadastramento\sdo\scodigo\sde\sacesso.\sO\sprocesso\sde\sautorizacao\sdeve\sser\sinterrompido\s(confirmar\scom\sa\sespecificacao);\so\scampo\sINFORMACOES_TELA\sserá\sdevolvido\spara\sa\sorigem;\so\scampo\sCRIPTOGRAMA\snão\sserá\sfornecido.
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="1" />
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="conta_nsgd_Type">
