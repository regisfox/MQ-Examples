				</xsd:simpleType>
			</xsd:element>
			<xsd:element minOccurs="0" name="FLAG_VALIDACAO">
				<xsd:annotation>
					<xsd:documentation source="WMQI_DOCUMENTATION">INDICA\sSE\sSERÁ\sNECESSARIO\sVALIDACAO\sDA\sIDENTIFICACAO\sPOSITIVA.\r\n\r\nDomínio:\r\nS\s-\sHA\sVALIDACAO.\r\nN\s-\sNAO\sHA\sVALIDACAO.
					</xsd:documentation>
